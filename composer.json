{
	"name": "mediawiki/mabs",
	"type": "mediawiki-extension",
	"license": "GPL-2.0-or-later",
	"extra": {
		"installer-name": "MABS"
	},
	"require-dev": {
		"jakub-onderka/php-parallel-lint": "~1.0",
		"mediawiki/mediawiki-codesniffer": "~20.0"
	},
	"scripts": {
		"fix": "phpcbf",
		"test": [
			"parallel-lint . --exclude vendor",
			"phpcs -p -s"
		]
	},
	"require": {
		"php": ">=7.0",
		"composer/installers": "~1.0",
		"gitonomy/gitlib": "~1.0"
	}
}
